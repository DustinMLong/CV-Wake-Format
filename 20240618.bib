	keywords = {Humans, HIV, United States/epidemiology, *HIV Infections/epidemiology, *Acquired Immunodeficiency Syndrome, Centers for Disease Control and Prevention, U.S., Community partnerships, Distributed data network, Public health, Schools, Surveillance, Universities},
