@article{spieler_trends_2022,
	title = {Trends in diabetes incidence and associated risk factors among people with {HIV} in the current treatment era.},
	volume = {36},
	copyright = {Copyright (c) 2022 Wolters Kluwer Health, Inc. All rights reserved.},
	issn = {1473-5571 0269-9370},
	doi = {10.1097/QAD.0000000000003348},
	abstract = {OBJECTIVE: To examine type 2 diabetes mellitus incidence and associated risk factors among people with HIV (PWH). DESIGN: A retrospective clinical cohort study of PWH at a Southeastern US academic HIV clinic between 2008 and 2018. METHODS: PWH who attended at least two clinic visits were evaluated with demographic and clinical data extracted from the electronic medical record (EMR). Diabetes was defined as: hemoglobin A1C {\textgreater}/=6.5\% and/or 2 glucose results {\textgreater}200 mg/dl (at least 30 days apart), diagnosis of diabetes in the EMR, or exposure to diabetes medication. Time to diabetes incidence was computed from the entire clinic population for each year. Multivariable Cox proportional hazard regression models with time-dependent covariates were created to evaluate the independent association between covariates and time to incident diabetes. RESULTS: Among 4113 PWH, we identified 252 incident cases of diabetes. Incidence increased from 1.04 incidents per 1000 person years (PY) in 2008, to 1.55 incidents per 1000 PY in 2018. Body mass index (hazard ratio [HR] 10.5 (6.2, 17.7)), liver disease (HR 1.9 (1.2, 3.1)), steroid exposure (HR 1.5 (1.1, 1.9)), and use of integrase inhibitors (HR 1.5 (1.1, 2.0)) were associated with incident diabetes. Additional associated factors included lower CD4 + cell counts, duration of HIV infection, exposure to nonstatin lipid-lowering therapy, and dyslipidemia. CONCLUSIONS: Rapidly increasing incident diabetes rates among PWH were associated with both traditional and HIV-related associated risk factors, particularly body weight, steroid exposure, and use of Integrase Inhibitors. Notably, several of the risk factors identified are modifiable and can be targeted for intervention.},
	language = {eng},
	number = {13},
	journal = {AIDS (London, England)},
	author = {Spieler, Gabriel and Westfall, Andrew O. and Long, Dustin M. and Cherrington, Andrea and Burkholder, Greer A. and Funderburg, Nicholas and Raper, James L. and Overton, Edgar T. and Willig, Amanda L.},
	month = nov,
	year = {2022},
	pmid = {35950938},
	pmcid = {PMC9529800},
	note = {Place: England},
	keywords = {Humans, Cohort Studies, Retrospective Studies, *Anti-HIV Agents/therapeutic use, *Diabetes Mellitus, Type 2/complications/drug therapy/epidemiology, *HIV Infections/complications/drug therapy/epidemiology, Glucose/therapeutic use, Glycated Hemoglobin, Incidence, Integrase Inhibitors, Lipids, Risk Factors},
