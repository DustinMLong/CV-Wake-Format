	pmcid = {PMC9875945},
	note = {Place: United States},
	keywords = {60K35, Cardiovascular Disease, Machine Learning, Penalized Regression, Primary 60K35, Random Forest, secondary 60K35, Survival},
	pages = {1847--1883},
}

@article{merlin_marijuana_2019,
	title = {Marijuana {Use} {Is} {Not} {Associated} {With} {Changes} in {Opioid} {Prescriptions} or {Pain} {Severity} {Among} {People} {Living} {With} {HIV} and {Chronic} {Pain}.},
	volume = {81},
	copyright = {All rights reserved},
	issn = {1944-7884 1525-4135},
	doi = {10.1097/QAI.0000000000001998},
	abstract = {BACKGROUND: People living with HIV (PLWH) commonly report marijuana use for chronic pain, although there is limited empirical evidence to support its use. There is hope that marijuana may reduce prescription opioid use. Our objective was to investigate whether marijuana use among PLWH who have chronic pain is associated with changes in pain severity and prescribed opioid use (prescribed opioid initiation and discontinuation). METHODS: Participants completed self-report measures of chronic pain and marijuana use at an index visit and were followed up for 1 year in the Center for AIDS Research Network of Integrated Clinical Systems (CNICS). Self-reported marijuana use was the exposure variable. Outcome variables were changes in pain and initiation or discontinuation of opioids during the study period. The relationship between exposure and outcomes was assessed using generalized linear models for pain and multivariable binary logistic regression models for opioid initiation/discontinuation. RESULTS: Of 433 PLWH and chronic pain, 28\% reported marijuana use in the past 3 months. Median pain severity at the index visit was 6.3/10 (interquartile range 4.7-8.0). Neither increases nor decreases in marijuana use were associated with changes in pain severity, and marijuana use was not associated with either lower odds of opioid initiation or higher odds of opioid discontinuation. CONCLUSIONS: We did not find evidence that marijuana use in PLWH is associated with improved pain outcomes or reduced opioid prescribing. This suggests that caution is warranted when counseling PLWH about potential benefits of recreational or medical marijuana.},
	language = {eng},
	number = {2},
	journal = {Journal of acquired immune deficiency syndromes (1999)},
	author = {Merlin, Jessica S. and Long, Dustin and Becker, William C. and Cachay, Edward R. and Christopolous, Katerina A. and Claborn, Kasey R. and Crane, Heidi M. and Edelman, Eva Jennifer and Lovejoy, Travis I. and Mathews, William Christopher and Morasco, Benjamin J. and Napravnik, Sonia and O'Cleirigh, Connall and Saag, Michael S. and Starrels, Joanna L. and Gross, Robert and Liebschutz, Jane M.},
	month = jun,
	year = {2019},
	pmid = {30865181},
	pmcid = {PMC6522289},
	note = {Place: United States},
	keywords = {Female, Humans, Prospective Studies, Male, United States, Middle Aged, Self Report, Surveys and Questionnaires, Follow-Up Studies, *Marijuana Use, Analgesics, Opioid/*therapeutic use, Chronic Pain/*drug therapy/etiology, HIV Infections/*complications, Logistic Models, Marijuana Smoking, Medical Marijuana/*therapeutic use, Multivariate Analysis, Opioid-Related Disorders, Prescription Drugs/*therapeutic use, Treatment Outcome},
	pages = {231--237},
}

@article{felicione_pilot_2019,
	title = {A pilot investigation of the effect of electronic cigarettes on smoking behavior among opioid-dependent smokers.},
	volume = {91},
	copyright = {Published by Elsevier Ltd.},
	issn = {1873-6327 0306-4603},
	doi = {10.1016/j.addbeh.2018.07.003},
	abstract = {INTRODUCTION: Compared to the general population, smoking rates are 2-4 times higher among individuals with opioid use disorders (OUDs). These smokers also have poor long-term cessation rates, even with pharmacotherapy or other interventions. Low success rates with traditional approaches may prompt smokers with OUDs to try more novel products like electronic cigarettes (ECIGs). This pilot study was designed to examine the feasibility, acceptability, and effect of ECIGs on smoking behavior among smokers with OUD. METHODS: Participants (N=25) were daily smokers receiving buprenorphine/naloxone for OUD at an outpatient clinic. They were randomized to use a second-generation ECIG (0 or 18ng/ml nicotine) ad libitum for two weeks while completing assessments via text messaging daily, and also via in-person visits at baseline, end of the two-week intervention, and a 4-week follow-up. RESULTS: Feasibility was evidenced by high enrollment (93.9\%) and retention (70.9\%) rates. ECIG adherence was relatively high as measured by self-report (80.6\% active, 91.7\% placebo), while the average volume of liquid used per week was low ({\textasciitilde}3ml). Both ECIG doses produced reductions in self-reported cigarettes per day that were not supported by average carbon monoxide levels. Biologically-confirmed smoking abstinence was observed in 8\% of participants. CONCLUSIONS: Preliminary results suggest that smokers with OUD are interested in using ECIGs, but their adherence may be less than ideal. Poor medication adherence rates are often observed in this disparate population, and future work should consider the use of other ECIG device types and a combination of methods to verify and quantify ECIG use.},
	language = {eng},
	journal = {Addictive behaviors},
	author = {Felicione, Nicholas J. and Enlow, Paul and Elswick, Daniel and Long, Dustin and Sullivan, C. R. and Blank, Melissa D.},
	month = apr,
	year = {2019},
	pmid = {30006020},
	note = {Place: England},
	keywords = {Female, Humans, Pilot Projects, Adult, Male, Young Adult, *Patient Compliance, Self Report, *Electronic Nicotine Delivery Systems, *Vaping, Buprenorphine, Buprenorphine, Naloxone Drug Combination/therapeutic use, Carbon Monoxide/metabolism, Cigarette Smoking/*drug therapy/metabolism, Electronic cigarette, Naloxone, Narcotic Antagonists/*therapeutic use, Nicotine/*therapeutic use, Nicotinic Agonists/*therapeutic use, Opiate Substitution Treatment, Opioid, Opioid-Related Disorders/complications/*drug therapy, Smoking},
