	note = {Place: United States},
	keywords = {Female, Humans, Prospective Studies, Adult, Male, Viral Load, Middle Aged, Analgesics, Opioid/*therapeutic use, Treatment Outcome, Chronic Pain/complications/*drug therapy, HIV Infections/complications/*drug therapy/virology},
	pages = {77--82},
}

@article{merlin_randomized_2018,
	title = {A {Randomized} {Pilot} {Trial} of a {Novel} {Behavioral} {Intervention} for {Chronic} {Pain} {Tailored} to {Individuals} with {HIV}.},
	volume = {22},
